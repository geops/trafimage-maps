{
  "name": "trafimage-maps",
  "version": "1.1.0-beta.1",
  "main": "build/bundle.js",
  "proxy": "http://127.0.0.1:8000",
  "dependencies": {
    "@datapunt/matomo-tracker-react": "^0.1.1",
    "@geops/create-react-web-component": "^2.0.19",
    "@geops/react-ui": "^1.0.10",
    "@turf/intersect": "^6.1.3",
    "@webcomponents/custom-elements": "1.3.2",
    "@webcomponents/webcomponents-platform": "^1.0.1",
    "@webcomponents/webcomponentsjs": "^2.4.2",
    "abortcontroller-polyfill": "^1.4.0",
    "color": "^3.1.2",
    "deepmerge": "^4.2.2",
    "i18next": "^19.3.3",
    "i18next-browser-languagedetector": "^4.0.2",
    "i18next-xhr-backend": "^3.2.2",
    "jsts": "2.1.0",
    "lodash": "^4.17.11",
    "mapbox-gl": "1.8.1",
    "node-sass": "^4.13.1",
    "ol": "6.1.1",
    "proj4": "^2.6.0",
    "prop-types": "^15.6.2",
    "proxy-polyfill": "^0.3.1",
    "query-string": "^6.11.1",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.3.4",
    "react-icons": "^3.9.0",
    "react-redux": "^7.2.0",
    "react-shadow": "^17.6.0",
    "react-spatial": "0.3.1",
    "react-styleguidist": "^11.0.4",
    "react-transit": "^0.3.3",
    "react-web-component": "^2.0.1",
    "redux": "^4.0.5",
    "redux-debounced": "^0.5.0",
    "redux-thunk": "^2.3.0",
    "style-it": "^2.1.4",
    "styled-components": "^5.0.1"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "babel-loader": "^8.0.6",
    "canvas": "^2.6.1",
    "cypress": "^4.3.0",
    "cypress-plugin-tab": "^1.0.5",
    "documentation": "^12.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "fetch-mock": "^9.3.1",
    "fixpack": "^3.0.6",
    "generact": "^0.4.0",
    "gensync": "^1.0.0-beta.1",
    "geops-docjs-template": "^1.0.2",
    "husky": "^4.2.3",
    "jest-canvas-mock": "^2.2.0",
    "jest-serializer-html": "^7.0.0",
    "lint-staged": "^10.0.9",
    "node-fetch": "^2.3.0",
    "prettier": "^2.0.2",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "^3.4.1",
    "react-svg-loader": "^3.0.3",
    "react-test-renderer": "^16.13.1",
    "react-web-component-style-loader": "^0.1.4-alpha",
    "redux-mock-store": "^1.5.4",
    "rewire": "4.0.1",
    "sort-json": "^2.0.0",
    "start-server-and-test": "^1.10.11",
    "stream-array": "^1.1.2",
    "stylelint": "^13.2.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.16.0",
    "to-string-loader": "^1.1.6",
    "vinyl-fs": "^3.0.3"
  },
  "scripts": {
    "build": "react-app-rewired build &&  bash ./scripts/build-es.sh && node scripts/prepare-package.js",
    "coverage": "yarn test  --watchAll=false  --coverage --coverageDirectory=coverage",
    "cp": "generact --root src/components",
    "cy:open": "cypress open",
    "cy:test": "start-server-and-test start http://localhost:3000 'cypress run'",
    "doc": "bash ./scripts/prepare-doc.sh",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.js' && eslint 'src/**/*.js' --fix && stylelint 'src/**/*.css' 'src/**/*.scss' --fix",
    "lint": "eslint 'src/**/*.js' && stylelint 'src/**/*.css' 'src/**/*.scss'",
    "prebuild": "rm -rf build/",
    "publish:alpha": "yarn publish:public --tag alpha",
    "publish:beta": "yarn publish:public --tag beta",
    "publish:public": "yarn version && git push origin HEAD && yarn run build && yarn publish build/ --access public --non-interactive --no-git-tag-version",
    "sort": "sort-json src/lang/*.json -i",
    "start": "react-app-rewired start",
    "start:doc": "styleguidist server",
    "test": "react-app-rewired test  --env=jsdom --transformIgnorePatterns \"/node_modules/(?!(@geops/react-ui|jsts|ol|react-spatial|react-transit)/).*/\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "config-overrides-path": "packages/wc/config-overrides.js",
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "linebreak-style": "Off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/forbid-prop-types": "Off",
      "prettier/prettier": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "husky": {
    "hooks": {
      "post-checkout": "yarn install --frozen-lockfile",
      "post-merge": "yarn install --frozen-lockfile",
      "post-rebase": "yarn install --frozen-lockfile",
      "pre-commit": "CI=true lint-staged   "
    }
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  },
  "lint-staged": {
    "src/lang/*.json": [
      "yarn sort",
      "git add"
    ],
    "(src|__mocks__)/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add",
      "yarn test --bail --findRelatedTests"
    ],
    "package.json": [
      "fixpack --sortToTop name --sortToTop description --sortToTop version --sortToTop author --sortToTop main --sortToTop proxy --sortToTop dependencies --sortToTop peerDependencies --sortToTop devDependencies --sortToTop resolutions --sortToTop scripts",
      "git add"
    ],
    "src/**/*.{scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recommended-scss"
    ],
    "rules": {
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "export"
          ]
        }
      ],
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "/transitiondelay/"
          ]
        }
      ]
    }
  }
}

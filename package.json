{
  "name": "trafimage-maps",
  "description": "trafimage-maps web component",
  "version": "1.6.5",
  "private": true,
  "main": "build/bundle.js",
  "proxy": "http://127.0.0.1:8000",
  "dependencies": {
    "@datapunt/matomo-tracker-react": "0.5.1",
    "@geops/create-react-web-component": "2.0.19",
    "@geops/react-ui": "1.1.5",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@turf/intersect": "6.5.0",
    "@webcomponents/custom-elements": "1.5.0",
    "@webcomponents/webcomponents-platform": "1.0.1",
    "@webcomponents/webcomponentsjs": "2.6.0",
    "abortcontroller-polyfill": "1.7.3",
    "cancelable-promise": "4.2.1",
    "canvas-size": "1.2.5",
    "canvg": "3.0.9",
    "color": "4.0.1",
    "deepmerge": "4.2.2",
    "i18next": "21.2.4",
    "i18next-browser-languagedetector": "6.1.2",
    "i18next-xhr-backend": "3.2.2",
    "jspdf": "2.4.0",
    "mapbox-gl": "1.13.1",
    "mobility-toolbox-js": "1.3.12",
    "node-sass": "6.0.1",
    "ol": "6.6.1",
    "proj4": "2.7.5",
    "prop-types": "15.7.2",
    "proxy-polyfill": "0.3.2",
    "query-string": "7.0.1",
    "re-resizable": "6.9.1",
    "react": "16.14.0",
    "react-app-polyfill": "2.0.0",
    "react-autosuggest": "10.1.0",
    "react-dom": "16.14.0",
    "react-i18next": "11.12.0",
    "react-icons": "4.3.1",
    "react-redux": "7.2.5",
    "react-select": "5.1.0",
    "react-spatial": "1.0.33",
    "react-styleguidist": "11.0.8",
    "react-web-component": "2.0.2",
    "redux": "4.1.1",
    "redux-debounced": "0.5.0",
    "redux-thunk": "2.3.0",
    "style-it": "2.1.4",
    "styled-components": "5.3.1"
  },
  "peerDependencies": {
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/eslint-parser": "7.15.7",
    "@svgr/webpack": "5.5.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-jest": "26.6.3",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "0.3.7",
    "babel-preset-react-app": "9",
    "canvas": "2.8.0",
    "cypress": "8.5.0",
    "cypress-plugin-tab": "1.0.5",
    "documentation": "13.2.5",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "fetch-mock": "9.11.0",
    "fixpack": "3.0.6",
    "generact": "0.4.0",
    "gensync": "1.0.0-beta.2",
    "geops-docjs-template": "1.0.4",
    "husky": "7.0.2",
    "jest-serializer-html": "7.1.0",
    "lint-staged": "11.2.0",
    "node-fetch": "3.0.0",
    "prettier": "2.4.1",
    "react-app-rewired": "2.1.8",
    "react-scripts": "4.0.3",
    "react-svg-loader": "3.0.3",
    "react-test-renderer": "17.0.2",
    "react-web-component-style-loader": "0.1.4-alpha",
    "redux-mock-store": "1.5.4",
    "sort-json": "2.0.0",
    "start-server-and-test": "1.14.0",
    "stream-array": "1.1.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended-scss": "4.3.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-scss": "3.21.0",
    "to-string-loader": "1.1.6",
    "vinyl-fs": "3.0.3",
    "xmlserializer": "^0.6.1"
  },
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false  react-app-rewired build &&  bash ./scripts/build-es.sh && node scripts/prepare-package.js",
    "coverage": "yarn test  --watchAll=false  --coverage --coverageDirectory=coverage",
    "cp": "generact --root src/components",
    "cy:open": "cypress open",
    "cy:test": "start-server-and-test start:http http://localhost:3000 'cypress run'",
    "doc": "bash ./scripts/prepare-doc.sh",
    "eject": "react-scripts eject",
    "fixpack": "fixpack --sortToTop name --sortToTop description --sortToTop version --sortToTop author --sortToTop private --sortToTop main --sortToTop proxy --sortToTop dependencies --sortToTop peerDependencies --sortToTop devDependencies --sortToTop resolutions --sortToTop scripts",
    "format": "prettier --write 'src/**/*.js' && eslint 'src/**/*.js' --fix && stylelint 'src/**/*.css' 'src/**/*.scss' --fix",
    "link2": "cmdToAdd=$(node ./scripts/read-pkg-json.js add) && $cmdToAdd && yarn build && cmdToRemove=$(node ./scripts/read-pkg-json.js remove) && $cmdToRemove && cd build && yarn link",
    "lint": "eslint 'src/**/*.js' && stylelint 'src/**/*.css' 'src/**/*.scss'",
    "prebuild": "rm -rf build/",
    "publish:alpha": "yarn publish:public --tag alpha",
    "publish:beta": "yarn publish:public --tag beta",
    "publish:public": "yarn version && git push origin HEAD && yarn run build && yarn publish build/ --access public --no-git-tag-version",
    "sort": "sort-json src/lang/*.json -i",
    "start": "ESLINT_NO_DEV_ERRORS=true HTTPS=true react-app-rewired start",
    "start:doc": "styleguidist server",
    "start:http": "ESLINT_NO_DEV_ERRORS=true react-app-rewired start",
    "test": "react-app-rewired test  --env=jsdom --transformIgnorePatterns \"/node_modules/(?!(node-fetch|fetch-blob|@geops/react-ui|jsts|ol|react-spatial|mobility-toolbox-js|ol-mapbox-style)/).*/\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "config-overrides-path": "packages/wc/config-overrides.js",
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parser": "@babel/eslint-parser",
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "linebreak-style": 0,
      "arrow-body-style": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/forbid-prop-types": "Off",
      "prettier/prettier": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  },
  "lint-staged": {
    "src/lang/*.json": [
      "yarn sort",
      "git add"
    ],
    "(src|__mocks__)/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add",
      "yarn test --bail --findRelatedTests"
    ],
    "src/**/*.scss": [
      "stylelint --fix",
      "git add"
    ],
    "package.json": [
      "yarn fixpack",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recommended-scss"
    ],
    "rules": {
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "export"
          ]
        }
      ],
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "/transitiondelay/"
          ]
        }
      ]
    }
  }
}

    "jest-serializer-html": "7.1.0",
    "lint-staged": "11.2.0",
    "mock-socket": "9.1.5",
    "prettier": "2.4.1",
    "react-app-rewired": "2.1.8",
    "react-error-overlay": "6.0.11",
    "react-scripts": "4.0.3",
    "react-svg-loader": "3.0.3",
    "react-test-renderer": "17.0.2",
    "react-web-component-style-loader": "0.1.4-alpha",
    "redux-mock-store": "1.5.4",
    "resemblejs": "4.1.0",
    "sass": "1.49.9",
    "sort-json": "2.0.1",
    "standard-version": "9.5.0",
    "start-server-and-test": "1.14.0",
    "stream-array": "1.1.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended-scss": "4.3.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-scss": "3.21.0",
    "to-string-loader": "1.2.0",
    "vinyl-fs": "3.0.3",
    "xmlserializer": "^0.6.1"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "scripts": {
    "build": "GENERATE_SOURCEMAP=false  react-app-rewired build &&  bash ./scripts/build-es.sh && node scripts/prepare-package.js",
    "coverage": "yarn test  --watchAll=false  --coverage --coverageDirectory=coverage",
    "cp": "generact --root src",
    "cy:open": "cypress open",
    "cy:test": "start-server-and-test start:http http://localhost:3000 'cypress run --browser chrome'",
    "cy:test:chrome": "yarn build && start-server-and-test start:http http://localhost:3000 'cypress run --browser chrome'",
    "cy:test:edge": "yarn build && start-server-and-test start:http http://localhost:3000 'cypress run --browser edge'",
    "cy:test:firefox": "yarn build && start-server-and-test start:http http://localhost:3000 'cypress run --browser firefox'",
    "doc": "bash ./scripts/prepare-doc.sh",
    "eject": "react-scripts eject",
    "fixpack": "fixpack --sortToTop name --sortToTop description --sortToTop version --sortToTop author --sortToTop private --sortToTop main --sortToTop proxy --sortToTop dependencies --sortToTop peerDependencies --sortToTop devDependencies --sortToTop resolutions --sortToTop scripts",
    "format": "prettier --write 'src/**/*.js' '*.md' && eslint 'src/**/*.js' --fix && stylelint 'src/**/*.css' 'src/**/*.scss' --fix",
    "link2": "cmdToAdd=$(node ./scripts/read-pkg-json.js add) && $cmdToAdd && yarn build && cmdToRemove=$(node ./scripts/read-pkg-json.js remove) && $cmdToRemove && cd build && yarn link",
    "lint": "eslint 'src/**/*.js' && stylelint 'src/**/*.css' 'src/**/*.scss'",
    "prebuild": "rm -rf build/",
    "prepare": "is-ci || husky install",
    "publish:alpha": "yarn release -- --prerelease alpha --skip.changelog && git push origin HEAD && yarn run build  && git push --tags && yarn publish build/ --access public --tag beta",
    "publish:beta": "yarn release -- --prerelease beta --skip.changelog && git push origin HEAD && yarn run build  && git push --tags && yarn publish build/ --access public --tag beta",
    "publish:beta:dryrun": "yarn release -- --prerelease beta --dry-run --skip.changelog",
    "publish:public": "yarn release && git push origin HEAD && yarn run build && git push --tags && yarn publish build/ --access public",
    "publish:public:dryrun": "yarn release --dry-run",
    "release": "standard-version",
    "sort": "sort-json src/lang/*.json -i",
    "start": "ESLINT_NO_DEV_ERRORS=true HTTPS=true react-app-rewired start",
    "start:doc": "ESLINT_NO_DEV_ERRORS=true  styleguidist server",
    "start:http": "ESLINT_NO_DEV_ERRORS=true react-app-rewired start",
    "test": "react-app-rewired test  --env=jsdom --transformIgnorePatterns \"/node_modules/(?!(node-fetch|fetch-blob|@geops/react-ui|jsts|ol|react-spatial|mobility-toolbox-js|ol-mapbox-style|geotiff|quick-lru)/).*/\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all",
    "not ie <= 11",
    "not android < 5"
  ],
  "config-overrides-path": "packages/wc/config-overrides.js",
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "jest": true,
      "cypress/globals": true
    },
    "parser": "@babel/eslint-parser",
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "prettier"
    ],
    "plugins": [
      "cypress",
      "prettier"
    ],
    "rules": {
      "linebreak-style": 0,
      "arrow-body-style": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/forbid-prop-types": "Off",
      "prettier/prettier": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "jest": {
    "resetMocks": false,
    "coverageReporters": [
      "text",
      "html"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  },
  "lint-staged": {
    "*.md": [
      "prettier --write"
    ],
    "src/lang/*.json": [
      "yarn sort",
      "git add"
    ],
    "(src|__mocks__)/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add",
      "yarn test --bail --findRelatedTests"
    ],
    "src/**/*.scss": [
      "stylelint --fix",
      "git add"
    ],
    "package.json": [
      "yarn fixpack",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recommended-scss"
    ],
    "rules": {
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "export"
          ]
        }
      ],
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "/transitiondelay/"
          ]
        }
      ]
    }
  }
}
